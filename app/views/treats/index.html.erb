<p id="notice"><%= notice %></p>

<h1>Listing Treats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Md</th>
      <th>Sym</th>
      <th>Reg</th>
      <th>User</th>
      <th>Cat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treats.each do |treat| %>
      <tr>
        <td><%= treat.name %></td>
        <td><%= treat.md %></td>
        <td><%= treat.sym %></td>
        <td><%= treat.reg %></td>
        <td><%= treat.user %></td>
        <td><%= treat.cat %></td>
        <td><%= link_to 'Show', treat %></td>
        <td><%= link_to 'Edit', edit_treat_path(treat) %></td>
        <td><%= link_to 'Destroy', treat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Treat', new_treat_path %>
